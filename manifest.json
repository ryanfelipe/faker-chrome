{
  "manifest_version": 2,
  "name": "Faker",
  "version": "0.1",
  "description": "Generate fake data in the browser",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background" : {
    "scripts": [
      "libs/faker.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png"
  }
}
